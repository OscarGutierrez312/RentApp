{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "module": "esnext",
  },
  "include": [
    "process.d.js",
    "next-env.d.js",
    "next-auth.d.js",
    "**/*.ts",
    "**/*.tsx"
, "components/header.js", "components/layout.js", "pages/api/auth/[...nextauth].js", "pages/api/auth/jwt.js", "pages/api/auth/protected.js", "pages/api/auth/session.js", "pages/_app.js", "pages/index.js"  ],
  "exclude": [
    "node_modules"
  ]
}
